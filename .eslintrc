{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [0],
    "no-tabs": [0],
    "no-mixed-spaces-and-tabs": [0],
    "react/jsx-props-no-spreading": [0]
  },
  "plugins": ["only-warn"]
}

