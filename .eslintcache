nfoWindow}>\n        <div>\n          <LocationsCard {...state.selectedPlace} />\n        </div>\n      </InfoWindow>\n    </Map>\n  );\n}\n\nexport default GoogleApiWrapper({\n  apiKey: process.env.REACT_APP_GOOGLE_MAPS_KEY,\n})(MapContainer);\n","/Users/leonvandeklundert/WILD/projects/project-3/app-frontend/src/components/Home/featuredLocations/FeaturedLocationsCard.js",["137","138","139","140","141"],"/Users/leonvandeklundert/WILD/projects/project-3/app-frontend/src/components/Locations/LocationsCard/LocationsCard.js",["142","143","144","145","146","147","148","149","150","151","152","153","154","155","156"],"/Users/leonvandeklundert/WILD/projects/project-3/app-frontend/src/contexts/LocationContext.js",["157","158","159","160"],"import React, { createContext, useState } from \"react\";\n\nexport const LocationContext = createContext();\n\nfunction LocationProvider(props) {\n  const [location, setLocation] = useState({});\n\n  return (\n    <LocationContext.Provider\n      value={{ location: location, setLocation: setLocation }}\n    >\n      {props.children}\n    </LocationContext.Provider>\n  );\n}\n\nexport default LocationProvider;\n",["161","162","163","164","165"],"/Users/leonvandeklundert/WILD/projects/project-3/app-frontend/src/components/Home/greeting/Greeting.js",[],"/Users/leonvandeklundert/WILD/projects/project-3/app-frontend/src/components/Home/communityUpdates/CommunityUpdatesCarousel.js",["166","167","168"],"/Users/leonvandeklundert/WILD/projects/project-3/app-frontend/src/components/Home/communityUpdates/CommunityUpdatesCard.js",["169","170","171","172","173","174"],"/Users/leonvandeklundert/WILD/projects/project-3/app-frontend/src/components/Home/featuredLocations/FeaturedLocationsRating.js",["175","176","177","178","179","180","181"],"import React from 'react';\n\nfunction FeaturedLocationsRating(props) {\n  const {rating} = props;\n  console.log(\"rating: \",rating);\n\n\n  return (\n    <div className=\"featured-locations-card__rating\">\n\n   