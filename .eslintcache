ord\"\n            name=\"password\"\n            placeholder={\n              !errors.password\n                ? \"Please enter your password\"\n                : \"repeat password\"\n            }\n            ref={register({\n              minLength: 8,\n              required: true,\n              pattern: /^(?=.*[0-9])(?=.*[!@#$%^&*])[a-zA-Z0-9!@#$%^&*]{8,20}$/i,\n            })}\n            onChange={handleChange}\n          />\n          <p>Country Code</p>\n          <input\n            type=\"number\"\n            name=\"phoneNumber\"\n            placeholder={\n              !errors.phoneNumber ? \"Phone Number is required\" : \"Phone Number\"\n            }\n            ref={register({ required: true, maxLength: 9 })}\n            onChange={handleChange}\n          />\n          <input\n            type=\"text\"\n            name=\"fullName\"\n            placeholder={\n              !errors.fullName ? \"Full Name is required\" : \"Full Name\"\n            }\n            ref={register({ required: true, maxLength: 30 })}\n            onChange={handleChange}\n          />\n          <input type=\"submit\" />\n        </form>\n      </div>\n    </div>\n  );\n}\nexport default Signup;\n","/home/luis/Documents/Wild Code School/Project Krow/project_krow/app-frontend/src/components/Home/greeting/GuestGreeting.js",[],"/home/luis/Documents/Wild Code School/Project Krow/project_krow/app-frontend/src/components/Home/greeting/UserGreeting.js",[],"/home/luis/Documents/Wild Code School/Project Krow/project_krow/app-frontend/src/components/Locations/LocationsCard/LocationsCard.js",["309","310","311","312","313","314","315","316","317","318","319","320","321","322","323"],"import React from \"react\";\n\nfunction LocationsCard(props) {\n  const checkoutClicked = () => console.log(\"checkedinn\");\n\n  return (\n    <div>\n      <h1>{props.name}</h1>\n      <img src={props.image} alt=\"image\" />\n      <div>\n        <p>Wifi Details</p>\n        <p>Network:{props.netwo