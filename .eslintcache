oute path=\"/membership\" component={Membership} />\n            <Route path=\"/signup\" component={Signup} />\n            <ProtectedRoute path=\"/profile\" component={MainProfile} />\n            {/* <Redirect to=\"/\" /> */}\n          </Switch>\n          <BottomNav />\n        </LocationProvider>\n      </AuthContextProvider>\n    </div>\n  );\n}\n\nexport default App;\n",["182","183","184","185","186"],"/home/luis/Documents/Wild Code School/Project Krow/project_krow/app-frontend/src/components/Navigation/BottomNav.js",["187","188","189"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\n\nconst BottomNav = () => {\n  return (\n    <div>\n      <Link to=\"/locations\">Map</Link>\n      {/*<Link to=\"/chat?\">Chat</Link>*/}\n      <Link to=\"/\">Homepage</Link>\n      <Link to=\"/profile/:id\">Profile</Link>\n\n\n    </div>\n  );\n};\nexport default BottomNav;",["190","191","192","193","194"],"/home/luis/Documents/Wild Code School/Project Krow/project_krow/app-frontend/src/components/Profile/Profile.jsx",["195","196","197","198","199","200","201","202","203","204","205"],"import React, { useState, useContext } from \"react\";\nimport { NavLink as Link, useHistory } from \"react-router-dom\";\nimport Cookies from \"js-cookie\";\nimport UserProfile from \"./UserProfile\";\nimport { AuthContext } from \"../../contexts/AuthContext\";\nimport { Content } from \"./Styles\";\n\nconst Profile = () => {\n  const { user, setUser } = useContext(AuthContext);\n  const history = useHistory();\n  const [isLoggedIn, setIsLoggedIn] = useState(true);\n\n  const LogOut = () => {\n    setIsLoggedIn(false);\n    history.push(\"/\");\n  };\n  return (\n    <Content>\n      <header>{isLoggedIn ? <UserProfile /> : <LogOut />}</header>\n      <div className=\"nav\">\n        <div className=\"multi-level\">\n          <div className=\"item\">\n            <input type=\"checkbox\" id=\"A\" />\n            <label className=\"label-top\" htmlFor=\"A\">\n           